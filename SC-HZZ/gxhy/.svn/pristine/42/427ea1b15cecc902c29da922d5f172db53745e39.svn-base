
.navbar-gxhy {
	margin-bottom: 0px;
	z-index:2000;
	position: fixed;
	top:0px;
	width: 100%;
	height: 65px;
	background-size: 100%;
}
.navbar-header{
		width:352px;
		height: 100%;
		line-height:65px;
	}
.navbar-right{
	margin-top: 12px;
}
.nav.navbar-onelv>li.active {
	background-color: #1D78D9;
	border-top:2px solid #78CD11;
	height: 94px !important;
	margin-top: -2px;
}

.nav>li>a:focus, .nav>li>a:hover{
	background-color: #57c9f3;
	color: #FFFFFF;
}
.nav .open>a, .nav .open>a:focus, .nav .open>a:hover{
	background-color: #57c9f3;
	color: #FFFFFF;
}
.dropdown-submenu {  
    position: relative;  
}  
  
.dropdown-submenu>.dropdown-menu {  
    -webkit-border-radius: 0 6px 6px 6px;  
    -moz-border-radius: 0 6px 6px;  
    border-radius: 0 6px 6px 6px;  
    left: 159px;
    right: auto;
    top: -3px;
    bottom: auto;
}  
  
.dropdown-submenu:hover>.dropdown-menu {  
    display: block;  
}  

/* .dropdown-menu > li > a:focus, .dropdown-menu > li > a:hover{ */
