river_lake_div_right.leftPanel_left{
	position: absolute;
    left: 0;
    width: 17.97%;
    /* border: solid 1px #bfbfbf; */
    background-color: #fcfcfc;
    border-right: 1px solid #bfbfbf;	
}
.river_lake_div_right{
	width: 82%;
	float: left;
}
.river_lake_div_left{
	float: left;

}
@media (min-width: 1366px){
	.leftPanel_left{
		 width: 20.97%;
	}
	.river_lake_div_right{
		width: 79%;
	}
	.river_lake_div_left{
		width: 15%;
	}
}
@media (min-width: 1920px){
	.leftPanel_left{
		 width: 20.97%;
	}
	.river_lake_div_right{
		width: 79%;
	}
	.river_lake_div_left{
		width: 15%;
	}
}
.form-control{
    display: block;
    width: 100%;
    height: 34px;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 4px;
}
.fa-search:before{
    content: "\f002";
}
.tab_container_head{
	display: table;
	width:100%;
	height:50px; 
}

.rdijit-layout-tab3 .tabItem{
    cursor: pointer;
}

.rdijit-layout-tab3 .iconItem{
    color: #a0a0a0;
    line-height:2em;
    display:table-cell;
    float:none;
    padding: 0;
    margin: 0;
    vertical-align: middle; 
    cursor:pointer;
    background-color: #edf1f5;
}
.rdijit-layout-tab3 .iconItem .fa{
	vertical-align: middle;
	padding-right: 5px; 
}

.rdijit-layout-tab3 .iconItem:hover{
    background-color: #edf1f5;
    color: #000;
    border-bottom-color: #edf1f5;
}
.rdijit-layout-tab3 .iconItem{
    border-bottom: solid 3px #fff;
}
.rdijit-layout-tab3 .iconItem.rdijit-state-selected{
	color: #337ab7;
    border-bottom-color: #337ab7;
    background-color: #ffffff;
}
.text-center{
	text-align: center;
}
