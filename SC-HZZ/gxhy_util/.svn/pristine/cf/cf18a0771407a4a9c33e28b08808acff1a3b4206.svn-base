package com.gxhy.base.util;

public class RvNmUtil {
	public static String F_RvNm = "第一总河长";
	public static String Z_RvNm = "总河长";
	public static String FZ_RvNm = "第一副总河长";
	public static String SZ_RvNm = "第二副总河长";
	public static String SX_RvNm = "水系总河长";
	public static String C_RvNm= "市级河长";
	public static String D_RvNm = "县级河长";
	public static String V_RvNm = "村级河长";
	
	
	/*
	 * 将type值转换为河长等级名称
	 */
	public static String getRvNm(String type) {
		 if (type == null || type.trim().equals(""))
             return null;
		 String RvNM = null;
	      switch(type) {
	      case "rv-1":
	    	  RvNM = F_RvNm;
	    	  break;
	      case "rv-2":
	    	  RvNM = Z_RvNm;
	    	  break;
	      case "rv-3":
	    	  RvNM = FZ_RvNm;
	    	  break;
	      case "rv-4":
	    	  RvNM = SZ_RvNm;
	    	  break;
	      case "rv-5":
	    	  RvNM = SX_RvNm;
	    	  break;
	      case "rv-6":
	    	  RvNM = C_RvNm;
	    	  break;
	      case "rv-7":
	    	  RvNM = D_RvNm;
	    	  break;
	      case "rv-8":
	    	  RvNM = V_RvNm;
	    	  break;
	      }
		return RvNM;
		
	}
}
