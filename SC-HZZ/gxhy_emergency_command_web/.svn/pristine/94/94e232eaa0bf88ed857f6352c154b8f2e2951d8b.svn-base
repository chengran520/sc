.spjk ul li{
	width:305px;
	height:120px;
	border: 1px solid #ccc;
	float: left;
	list-style-type: none;
}
.spjk ul li img{
	margin-top:20px;
}
.spjk ul li p{
	font-size:15px;
	height:40px;
	line-height: 40px;
}
#head_div{
	float: left;
	margin-top:10px;
	margin-left:20px;
}
#head_div img{
	height:700px;
	float: left;
	border:4px solid #366CC6;
}
#middle_div{
	float: left;
	margin-top:10px;
	margin-right:20px;
	border-left:20px solid #366CC6;
}
#middle_div .mide-div div{
	float: left;
}
#middle_div .mide-div div img{
	width:370px;
	height:350px;
	float: left;
	border:4px solid #366CC6;
}
.red{
	background-color:#0ab1f8;
	color:#fff;
}
#tubia li{
	cursor: pointer;
}